!def(hello name) {
    print Hello, $(cat $name .)
}

!def(double x) {
    set y 1000
    return $(* $x $x)
}

!def(factorial n) {
    !if($(<= $n 1)) {
        return 1
    }

    return $(* $(factorial $(- $n 1)) $n)
}

hello world
set x 100
set y 50
print y is $y
print x is $x
print 5 doubled is $(double 5)
print y is still $y
print x is still $x

print Recursive functions are supported: factorial of 6 is $(factorial 6)